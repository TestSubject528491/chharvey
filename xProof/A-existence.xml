<?xml version="1.0" encoding="UTF-8"?>
<theorem xmlns="http://chharvey.net16.net/xProof/material.xsd" id="axiom-existence">
	<name>The Axiom of Existence</name>
	<summary>There exists an empty set, and that set is unique.</summary>
	<statement model="material" isAxiom="true">
		<quantifier type="existential">
			<setdef id="x"/>
			<predicate>
				<summary><math>x</math> is empty and unique</summary>
				<conjunction>
					<operand>
						<summary>\(x\) is empty</summary>
						<negation>
							<quantifier type="existential">
								<setdef id="t1"/>
								<predicate>
									<relation>
										<childset ref="t1"/>
										<parentset ref="x"/>
									</relation>
								</predicate>predicate>
							</quantifier>
						</negation>
					</operand>
					<operand>
						<summary>\(x\) is unique</summary>
						<quantifier type="universal">
							<setdef id="y"/>
							<predicate>
								<summary>if <math>y</math> is empty then <math>y=x</math></summary>
								<implication>
									<antecedent>
										<summary><math>y</math> is empty</summary>
										<negation>
											<quantifier type="existential">
												<setdef id="t2"/>
												<predicate>
													<relation>
														<childset ref="t2"/>
														<parentset ref="y"/>
													</relation>
												</predicate>
											</quantifier>
										</negation>
									</antecedent>
									<consequent>
										<summary><math>y=x</math></summary>
										<quantifier type="existential">
											<setdef id="t3"/>
											<predicate>
												<biconditional>
													<antecedent>
														<relation>
															<childset ref="t3"/>
															<parentset ref="y"/>
														</relation>
													</antecedent>
													<consequent>
														<relation>
															<childset ref="t3"/>
															<parentset ref="x"/>
														</relation>
													</consequent>
												</biconditional>
											</predicate>
										</quantifier>
									</consequent>
								</implication>
							</predicate>
						</quantifier>
					</operand>
				</conjunction>
			</predicate>
		</quantifier>
	</statement>
</theorem>



