<?xml version="1.0" encoding="UTF-8"?>
<theorem xmlns="http://chharvey.net16.net/xProof/logical.xsd" id="OR-introduction">
	<name>Disjunction Introduction</name>
	<!--  assume P : conclude (P and Q)  -->
	<statement model="logical" isAxiom="true">
		<propdefs>
			<propdef id="p"/>
			<propdef id="q"/>
		</propdefs>
		<premises>
			<premise>
				<prop ref="p"/>
			</premise>
		</premises>
		<conclusion>
			<disjunction>
				<operand><prop ref="p"/></operand>
				<operand><prop ref="q"/></operand>
			</disjunction>
		</conclusion>
	</statement>
</theorem>
