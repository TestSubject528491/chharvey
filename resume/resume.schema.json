{
  "$schema": "http://json-schema.org/schema#",
  "$id": "http://chharvey.github.io/resume/resume.schema.json",
  "title": "Resume",
  "description": "Resume of Christopher H. Harvey of Fairfax, VA.",
  "definitions": {},
  "type": "object",
  "required": ["contact"],
  "additionalProperties": false,
  "properties": {
    "contact": {
      "description": "List of contact points.",
      "type": "array",
      "items": {
        "type": "object",
        "required": ["url", "octicon", "content"],
        "additionalProperties": false,
        "properties": {
          "url"     : { "type": "string", "description": "url of the link" },
          "octicon" : { "type": "string", "description": "octicon CSS class of the icon" },
          "itemprop": { "type": "string", "description": "(optional) itemprop Microdata of the contact point" },
          "content" : { "type": "string", "description": "text content of the link" }
        }
      }
    },
    "skills": {},
    "positions": {},
    "degrees": {},
    "prodevs": {},
    "awards": {},
    "teams": {}
  }
}
