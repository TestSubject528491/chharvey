<?xml version="1.0" encoding="UTF-8"?>
<theorem-logical xmlns="http://chharvey.net16.net/ProofML/logical/proofml-logical.xsd">
	<name>Disjunction Introduction</name>
	<comment>assume P : conclude (P or Q)</comment>
	<statement model="logical" isAxiom="true">
		<propdefs>
			<propdef id="p"/>
			<propdef id="q"/>
		</propdefs>
		<premises>
			<premise>
				<prop ref="p"/>
			</premise>
		</premises>
		<conclusion>
			<disjunction>
				<operand><prop ref="p"/></operand>
				<operand><prop ref="q"/></operand>
			</disjunction>
		</conclusion>
	</statement>
</theorem-logical>
