{
  "name": "chharvey.github.io",
  "version": "4.0.1",
  "description": "My personal web site.",
  "private": true,
  "main": "index.html",
  "dependencies": {},
  "devDependencies": {
    "bangs": "^0.8.0",
    "jade": "^1.11.0",
    "less": "^2.6.0",
    "lux-nox": "^1.1.0",
    "octicons": "^3.1.0",
    "pug-cli": "^1.0.0-alpha6",
    "sitepage": "^1.0.0",
    "still-alive": "git://github.com/chharvey/still-alive.git#v1.0.0",
    "xmeter": "^5.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "jade:landing": "pug index.jade",
    "pug:home": "pug -O home/_models/options.js home/{about,copyright,cover-letter,math,edu,music,swim,web}.jade",
    "jade:resume": "pug resume/resume.jade",
    "jade:blog": "pug -O blog/_models/options.js blog/{classical-music,coffee,griddemo,headings,html-entities,iexplore,index,layout,lists,math-changing-world,math-moodle,page-relationships,rainbow-bridge,references-edu,references-mathed,semantic-punctuation,sort,workflow-git}.jade",
    "jade:all": "npm run jade:landing  &&  npm run jade:home  &&  npm run jade:resume  &&  npm run jade:blog",
    "lessc": "lessc",
    "lessc:landing": "lessc  ./home/styles/landing.less  ./home/styles/landing.css",
    "lessc:home": "lessc  ./home/styles/home.less  ./home/styles/home.css",
    "lessc:resume": "lessc  ./resume/styles/resume.less  ./resume/styles/resume.css",
    "lessc:all": "npm run lessc:landing  &&  npm run lessc:home  &&  npm run lessc:resume",
    "lessc:highered": "lessc  ./higher-ed/highered.less  ./higher-ed/highered.css",
    "lessc:secondaryed": "lessc  ./secondary-ed/secondaryed.less  ./secondary-ed/secondaryed.css",
    "ub:core": "rm home/styles/landing.css",
    "ub:home": "rm home/{about,copyright,cover-letter,edu,math,music,swim,web}.html home/styles/home.css",
    "ub:resume": "rm resume/resume.html resume/styles/resume.css",
    "ub:blog": "rm blog/{classical-music,griddemo,headings,html-entities,index,layout,lists,math-changing-world,math-moodle,page-relationships,references-edu,references-mathed,semantic-punctuation,sort,workflow-git}.html",
    "unbuild": "npm run ub:core  &&  npm run ub:home  &&   npm run ub:resume  &&   npm run ub:blog",
    "build": "npm run jade:all  &&  npm run lessc:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chharvey/chharvey.github.io.git"
  },
  "keywords": [
    "portfolio",
    "web",
    "dev",
    "math",
    "music",
    "education",
    "teaching"
  ],
  "author": "Chris Harvey <chrisharvey2pi@gmail.com> (https://chharvey.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chharvey/chharvey.github.io/issues"
  },
  "homepage": "https://chharvey.github.io/"
}
